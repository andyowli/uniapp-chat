<view class="contain"><live-pusher class="livePusher vue-ref" id="livePusher" url mode="SD" muted="{{true}}" enable-camera="{{true}}" auto-focus="{{true}}" beauty="{{1}}" whiteness="2" aspect="9:16" data-ref="livePusher" data-event-opts="{{[['statechange',[['statechange',['$event']]]],['netstatus',[['netstatus',['$event']]]],['error',[['error',['$event']]]]]}}" bindstatechange="__e" bindnetstatus="__e" binderror="__e"></live-pusher><button data-event-opts="{{[['tap',[['start',['$event']]]]]}}" class="btn" bindtap="__e">开始推流</button><button data-event-opts="{{[['tap',[['pause',['$event']]]]]}}" class="btn" bindtap="__e">暂停推流</button><button data-event-opts="{{[['tap',[['resume',['$event']]]]]}}" class="btn" bindtap="__e">resume</button><button data-event-opts="{{[['tap',[['stop',['$event']]]]]}}" class="btn" bindtap="__e">停止推流</button><button data-event-opts="{{[['tap',[['snapshot',['$event']]]]]}}" class="btn" bindtap="__e">快照</button><button data-event-opts="{{[['tap',[['startPreview',['$event']]]]]}}" class="btn" bindtap="__e">开启摄像头预览</button><button data-event-opts="{{[['tap',[['stopPreview',['$event']]]]]}}" class="btn" bindtap="__e">关闭摄像头预览</button><button data-event-opts="{{[['tap',[['switchCamera',['$event']]]]]}}" class="btn" bindtap="__e">切换摄像头</button></view>